  "scripts": {
    "dev": "cross-env MODE=dev IS_DEV=dev NODE_ENV=development node ./.profile/start.js",
    "build": "cross-env MODE=prod NODE_ENV=production webpack --config ./.profile/webpack.prod.config.js",
    "build:analyze": "cross-env IS_ANALYZE=analyze NODE_ENV=production webpack --config ./.profile/webpack.prod.config.js",
